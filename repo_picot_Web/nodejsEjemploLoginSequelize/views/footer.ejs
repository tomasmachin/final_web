<% if (message) { %>
    <div class="alert alert-primary alert-dismissible" role="alert">
    <%- message %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } if (error) { %>
    <div class="alert alert-danger alert-dismissible" role="alert">
    <%- error %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } %>
</body>
</html>